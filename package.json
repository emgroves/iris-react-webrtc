{
  "name": "iris-react-webrtc",
  "version": "1.0.13",
  "description": "React components for Iris WebRTC library",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist src",
    "build:umd": "webpack --output-filename iris.react.webrtc.js --config .webpack.config.babel.js",
    "build:umd.min": "webpack --output-filename iris.react.webrtc.min.js -p --config .webpack.config.babel.js"
  },
  "jest": {
    "automock": false,
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./setup-jasmine-env.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:iris-platform/iris-react-webrtc.git"
  },
  "keywords": [
    "iris",
    "react",
    "webrtc"
  ],
  "author": "Rob Sliwa <robjsliwa@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/iris-platform/iris-react-webrtc",
  "dependencies": {
    "keymirror": "^0.1.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-jest": "14.1.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.11.0",
    "chai": "^3.5.0",
    "jasmine-reporters": "2.2.0",
    "jest": "14.1.0",
    "jquery": "3.1.0",
    "json-loader": "0.5.4",
    "npm-run-all": "2.3.0",
    "react-test-renderer": "15.3.0",
    "rimraf": "2.5.3",
    "webpack": "1.13.1"
  }
}
